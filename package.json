{
  "name": "aqa-asm",
  "displayName": "AQA_ASM",
  "description": "Syntax highlighter for AQA assembly code",
  "version": "0.0.1",
  "engines": {
    "vscode": "^1.101.0"
  },
  "categories": [
    "Programming Languages"
  ],
  "contributes": {
    "languages": [{
      "id": "asm",
      "aliases": ["AQA Assembly", "asm"],
      "extensions": [".asm",".a",".as"],
      "configuration": "./language-configuration.json"
    }],
    "grammars": [{
      "language": "asm",
      "scopeName": "source.asm",
      "path": "./syntaxes/asm.tmLanguage.json"
    }]
  }
}
